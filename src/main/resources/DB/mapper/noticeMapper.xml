<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.iu.s5.notice.NoticeDAO">
  
  	<insert id="boardWrite" parameterType="com.iu.s5.notice.NoticeDTO">
  	insert into notice values(board_seq.nextval,#{title},#{writer},#{contents},sysDate,0)
  	</insert>
  	
  	<delete id="boardDelete" parameterType="java.lang.Long"><!--념겨주는 변수명 -->
  	delete notice where num=#{num}
  	</delete>
  	
  	<update id="boardUpdate" parameterType="com.iu.s5.notice.NoticeDTO">
  	update notice set title=#{title},writer=#{writer},contents=#{contents} where num=#{num}
  	</update>
  	
  	<update id="boardUpdate2" parameterType="com.iu.s5.notice.NoticeDTO">
  	update notice set hit=#{hit} where num=#{num}
  	</update>


  </mapper>
